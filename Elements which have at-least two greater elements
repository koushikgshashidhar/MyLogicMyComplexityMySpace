public class Solution {
    public ArrayList<Integer> solve(ArrayList<Integer> A) {
        
       /* using std library
       A.remove(Collections.max(A));
        A.remove(Collections.max(A));*/
        
        //assign 1stmax and 2nd max to Integer.MIN_VALUE
        int firstMax=Integer.MIN_VALUE;
        int secondMax=Integer.MIN_VALUE;
        //loop arraylist to get 1stmax and 2nd max
        for(int a:A)
        {
            if(a>firstMax)
            {
                secondMax=firstMax;
                firstMax=a;
                
            }
            else if(a>secondMax ){
               secondMax=a; 
            }
            
        }
        //remove 1stmax and 2nd max from arraylist and return it;
        A.remove((Integer)firstMax);
        A.remove((Integer)secondMax);
        return A;
    }
}

